set {
	reference rhf
	basis cc-pvtz
}


set_num_threads( 4 )
memory 2 gb

molecule MOL {
0 1

H	 2.416493	 -0.708526	 0.353417
C	 1.692903	 0.085809	 0.426094
O	 0.462329	 -0.453336	 -0.000137
H	 2.000343	 0.906222	 -0.210005
H	 1.636159	 0.422267	 1.453622
